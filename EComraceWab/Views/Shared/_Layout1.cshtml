    $(function () {
        $("#btnLogin").click(function () {
            debugger
            var email = $('#Email').val();
            var password = $('#Password').val();

            $(".error").remove();
            if (email.length < 1) {
                $('#Email').after('<span class="error text-danger">This Email is required</span>');
                return;
            }

            if (password.length < 8) {
                $('#Password').after('<span class="error text-danger">Password must be at least 8 characters long</span>');
                return;
            }
            var data = {
                Email: $("#Email").val(),
                Password: $("#Password").val(),
            };
            console.log(data);
            $.ajax({
                type: 'POST',
                url: "/User/Authenticate",
                data,
                success: function (response) {
                    debugger
                    console.log(response);
                    //$("#User").hide();
                    //$("#UserName").append("<span class='ms-1 d-none d-sm-inline-block'>" + response + "</span>");
                    debugger
                    sessionStorage.setItem('Token', response.Token);
                    UserView();

                },

                error: function () { alert("Something went wrong"); },
            });
        });

    });
    function UserView() {
        var token = sessionStorage.getItem('Token');
        $.ajax({
            type: "Get",
            headers: {
                Authorization: 'Bearer ' + token
            },
            url: "/Admain/GetId",
            success: function (response) {
                debugger
                console.log(response);
                if (response.RolesId == 2) {
                    window.location.href = '/Home/SallerHomePage?Id=' + response.Id;

                }
                else if (response.RolesId == 3) {
                    window.location.href = '/Daraz/Home';
                } else if (response.RolesId == 1) {
                    window.location.href = '/Admain/Wellcome';
                } else if (response.RolesId == 4) {
                    window.location.href = '/Admain/Wellcome?Id=' + response.RolesId;
                }
            }
        });
    }
